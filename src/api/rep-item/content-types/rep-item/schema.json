{
  "kind": "collectionType",
  "collectionName": "rep_items",
  "info": {
    "singularName": "rep-item",
    "pluralName": "rep-items",
    "displayName": "RepItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "link": {
      "type": "string"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "quality": {
      "type": "enumeration",
      "enum": [
        "High",
        "Middle",
        "Low"
      ],
      "default": "Middle",
      "required": true
    },
    "logo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::logo.logo",
      "inversedBy": "rep_items"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::seller.seller",
      "inversedBy": "rep_items"
    }
  }
}
